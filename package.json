{
  "name": "studio-elan",
  "version": "1.0.0",
  "description": "Studio de yoga moderne avec système de réservation en ligne",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --max-warnings=0 --ignore-pattern \"src/**\" \"app/**/*.{ts,tsx}\" \"components/**/*.{ts,tsx}\" \"hooks/**/*.{ts,tsx}\" \"lib/**/*.{ts,tsx}\" \"__tests__/**/*.{ts,tsx}\" \"types/**/*.{ts,tsx}\" \"WebVitalsMonitor.tsx\" --no-error-on-unmatched-pattern",
    "lint:next": "next lint",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:front": "prettier --write \"{app,components,hooks,lib,__tests__,types}/**/*.{ts,tsx,js,jsx,css,md}\" WebVitalsMonitor.tsx",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:backend": "jest -c jest.backend.config.js --passWithNoTests",
    "test:backend:watch": "jest -c jest.backend.config.js --watch",
    "test:backend:coverage": "jest -c jest.backend.config.js --coverage",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "analyze": "ANALYZE=true npm run build",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "node prisma/seed.js",
    "db:studio": "prisma studio",
    "postinstall": "prisma generate",
    "prepare": "husky install",
    "lint:fix": "eslint --fix --ignore-pattern \"src/**\" \"app/**/*.{ts,tsx}\" \"components/**/*.{ts,tsx}\" \"hooks/**/*.{ts,tsx}\" \"lib/**/*.{ts,tsx}\" \"__tests__/**/*.{ts,tsx}\" \"types/**/*.{ts,tsx}\" \"WebVitalsMonitor.tsx\" --no-error-on-unmatched-pattern"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@hookform/resolvers": "^3.3.2",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.6.0",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.1.5",
    "@tanstack/react-query": "^5.8.4",
    "@tanstack/react-query-devtools": "^5.8.4",
    "bcrypt": "^5.1.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "lucide-react": "^0.294.0",
    "next": "14.0.3",
    "next-auth": "4.24.7",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "sonner": "^1.2.4",
    "stripe": "^14.7.0",
    "tailwind-merge": "^2.0.0",
    "tailwindcss-animate": "^1.0.7",
    "web-vitals": "^3.5.0",
    "zod": "^3.22.4",
    "axios": "^1.6.5",
    "next-themes": "^0.2.1",
    "framer-motion": "^10.18.0",
    "react-big-calendar": "^1.11.2",
    "date-fns": "^2.30.0",
    "@stripe/stripe-js": "^2.1.0",
    "@sentry/nextjs": "^7.80.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.0.3",
    "@playwright/test": "^1.40.1",
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@types/pg": "^8.10.7",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.54.0",
    "eslint-config-next": "14.0.3",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "prisma": "^5.6.0",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "@testing-library/react": "^14.0.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/user-event": "^14.5.2",
    "whatwg-fetch": "^3.6.20",
    "@types/react-big-calendar": "^1.8.7",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.0",
    "ts-jest": "^29.1.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix --max-warnings=0 --ignore-pattern \"src/**\" --no-error-on-unmatched-pattern"
    ],
    "*.{json,md,css,scss}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "yoga",
    "studio",
    "reservation",
    "nextjs",
    "react",
    "typescript",
    "tailwindcss",
    "prisma"
  ],
  "author": "Studio Élan",
  "license": "MIT"
}

